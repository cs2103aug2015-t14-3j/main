
public class Parser {
	
	final String ERROR_COMMAND = "Command not found. Please enter 'help' for more information.";
	
	String userInput;
	
	public Parser(String input) {
		userInput = input;
	}
	
	String getCommand () {
		if (userInput.contains("add")) {
			return "add";
		} else if (userInput.contains("delete")) {
			return "delete";
		} else if (userInput.contains("display")) {
			return "display";
		} else if (userInput.contains("edit")) {
			return "edit";
		} else if (userInput.contains("search")) {
			return "search";
		} else if (userInput.contains("undo")) {
			return "undo";
		} else if (userInput.contains("help")) {
			return "help";
		} else if (userInput.contains("edit")) {
			return "edit";
		}
		return ERROR_COMMAND;
	} 

}
